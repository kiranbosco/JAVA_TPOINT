package com.unray;

public class ConstructorChaining {

	int id;
	String address;
	int age;
	String name;
	
	public ConstructorChaining(int id, String string, String string2, int i) {
		this.age = age;
	}
	public ConstructorChaining(int id , int age) {
		this(i);
		this.age = age;
	}
	public ConstructorChaining(int id, int age, String address,String  name) {
		
		this(id,age);
		this.address = address;
		this.name = name;
		
	}
	void disp() {
		System.out.println(id + " "+ name + " " + address  + " "+ age);
	}
	public static void main(String[] args) {
ConstructorChaining chs = new ConstructorChaining(10 ,"kiran","bng",5);	
}
	
}
