package com.oops;

// Agrigation exampel code
class Address {
	String streetNum;
	String city;
	String state;
	String country;

	public Address(String streetNum, String city, String state, String country) {
		this.streetNum = streetNum;
		this.streetNum = city;
		this.state = state;
		this.country = country;
	}
	class StudentClass {
		int rollNum;
		String studentName;
		//Creating HAS-A relationship with Address class
		Address studentAddr;  
		StudentClass(int roll, String name, Address studentAddr){
			this.rollNum = rollNum;
			this.studentName = studentName;
			this.studentAddr =studentAddr;

		}
		public void disp() {
			System.out.println(studentAddr + " "+ rollNum + " "+ studentName );
		}
	} 
	public static void main(String[] args) {
    
		  Address ad = new Address("churchColony", "nellore", "ap", "india");
		  StudentClass cl = new StudentClass(10, "charan", a);


	}
}
